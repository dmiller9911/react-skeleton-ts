{
  "name": "react-skeleton-ts",
  "version": "1.0.0",
  "description": "Skelton for a React Application using typescript.  Includes webpack, jest, enzyme.",
  "main": "src/index.js",
  "scripts": {
    "lint": "tslint src/**/*.ts src/**/*.tsx",
    "start": "node scripts/server",
    "test": "jest"
  },
  "keywords": [
    "TypeScript",
    "React",
    "Webpack",
    "Jest",
    "Aphrodite"
  ],
  "author": "Doug Miller <doug@dougmiller.io>",
  "license": "MIT",
  "dependencies": {
    "@types/aphrodite": "^0.5.4",
    "@types/enzyme": "^2.5.37",
    "@types/history": "^2.0.39",
    "@types/jest": "^15.1.32",
    "@types/react": "^0.14.47",
    "@types/react-dom": "^0.14.18",
    "@types/react-router": "^2.0.38",
    "@types/webfontloader": "^1.6.27",
    "@types/webpack": "^1.12.35",
    "@types/webpack-env": "^1.12.1",
    "aphrodite": "^0.5.0",
    "autoprefixer": "^6.4.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "enzyme-to-json": "^1.3.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "jest-cli": "^17.0.2",
    "json-loader": "^0.5.4",
    "normalize.css": "^5.0.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.2.0",
    "postcss-loader": "^0.13.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router": "^3.0.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "tslint-eslint-rules": "^1.5.0",
    "tslint-loader": "^2.1.5",
    "tslint-react": "^1.0.0",
    "typescript": "^2.1.1",
    "webfontloader": "^1.6.26",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-fail-plugin": "^1.0.5"
  },
  "jest": {
    "automock": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "notify": true,
    "transform": {
      ".*": "<rootDir>/testPreprocessor.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/testEnvironment.js",
    "testRegex": "spec.tsx?$",
    "unmockedModulePathPatterns": [
      "enzyme",
      "react",
      "aphrodite",
      "<rootDir>/testing"
    ]
  }
}
